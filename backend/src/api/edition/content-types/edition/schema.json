{
  "kind": "collectionType",
  "collectionName": "editions",
  "info": {
    "singularName": "edition",
    "pluralName": "editions",
    "displayName": "Edition"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "file_hash": {
      "type": "string"
    },
    "edition_number": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "year": {
      "type": "integer",
      "required": true
    },
    "work": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::work.work"
    },
    "pdf_file": {
      "allowedTypes": [
        "files"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    }
  }
}
