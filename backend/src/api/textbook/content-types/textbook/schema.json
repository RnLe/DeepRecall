{
  "kind": "collectionType",
  "collectionName": "textbooks",
  "info": {
    "singularName": "textbook",
    "pluralName": "textbooks",
    "displayName": "Textbook",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "isbn": {
      "type": "string"
    },
    "doi": {
      "type": "string"
    },
    "authors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::author.author"
    },
    "textbook_versions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::textbook-version.textbook-version",
      "mappedBy": "textbook"
    }
  }
}
