{
  "kind": "collectionType",
  "collectionName": "paper_versions",
  "info": {
    "singularName": "paper-version",
    "pluralName": "paper-versions",
    "displayName": "PaperVersion",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "file_hash": {
      "type": "string"
    },
    "pdf_file": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "version_number": {
      "type": "string"
    },
    "year": {
      "type": "integer",
      "required": true
    },
    "volume": {
      "type": "string"
    },
    "pages": {
      "type": "string"
    },
    "paper": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::paper.paper",
      "inversedBy": "paper_versions"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "page_count": {
      "type": "integer",
      "required": true
    },
    "file_size": {
      "type": "integer",
      "required": true
    }
