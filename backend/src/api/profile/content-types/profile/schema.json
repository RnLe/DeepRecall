{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Username": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "ProfilePicture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "Providers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::provider.provider",
      "mappedBy": "Profile"
    }
  }
}
