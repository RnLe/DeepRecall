{
  "kind": "collectionType",
  "collectionName": "textbook_versions",
  "info": {
    "singularName": "textbook-version",
    "pluralName": "textbook-versions",
    "displayName": "TextbookVersion",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "file_hash": {
      "type": "string"
    },
    "edition_number": {
      "type": "biginteger",
      "required": true
    },
    "year": {
      "type": "biginteger",
      "required": true
    },
    "pdf_file": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "textbook": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::textbook.textbook",
      "inversedBy": "textbook_versions"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "page_count": {
      "type": "integer",
      "required": true
    },
    "file_size": {
      "type": "integer",
      "required": true
    }
  }
}
